@{
    ViewData["Title"] = "AllExams";
    @model IEnumerable<OnlineExamer.Models.Exams.ExamViewModel>
}

<h1 class="exams-header">Всички изпити</h1>

<div class="container">
    <div class="row">
            @foreach (var exam in this.Model)
            {
                <button class="container col-md-4 card card-button">
                    @exam.ExamType
                </button>
            }
    </div>
</div>